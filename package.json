{
  "name": "y",
  "displayName": "scpedReplace",
  "description": "Enhances VS Code's Find feature by automatically restricting search to the selected range.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scopedReplace.smartFind",
        "title": "Smart Find"
      }
    ],
    "keybindings": [
      {
        "command": "-actions.find",
        "key": "ctrl+f",
        "when": "editorTextFocus"
      },
      {
        "command": "scpedReplace.smartFind",
        "key": "ctrl+f",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
